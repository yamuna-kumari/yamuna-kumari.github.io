{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "prefer-arrow-callback": "error",
    "prefer-template": "error",
    "prefer-const": "warn",
    "prefer-destructuring": "warn",
    "no-unused-vars": "warn",
    "react/react-in-jsx-scope": "off"
  },
  "plugins": ["prettier", "react", "react-hooks"]
}
